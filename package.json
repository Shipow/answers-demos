{
  "private": true,
  "name": "preact",
  "version": "0.0.0",
  "license": "MIT",
  "main": "src/index.js",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run -s serve || npm run -s dev",
    "build": "preact build",
    "serve": "preact build && preact serve",
    "dev": "preact watch",
    "lint": "eslint src",
    "test": "jest ./tests"
  },
  "eslintConfig": {
    "extends": "eslint-config-synacor"
  },
  "eslintIgnore": [
    "build/*"
  ],
  "devDependencies": {
    "eslint-config-synacor": "^1.1.0",
    "eslint": "^4.5.0",
    "if-env": "^1.0.0",
    "less-loader": "^4.0.5",
    "less": "^2.7.2",
    "node-sass": "^4.5.3",
    "preact-cli": "^1.4.1",
    "sass-loader": "^6.0.6",
    "stylus-loader": "^3.0.1",
    "stylus": "^0.54.5"
  },
  "dependencies": {
    "@algolia/autocomplete-js": "https://pkg.csb.dev/algolia/autocomplete.js/commit/f945a87e/@algolia/autocomplete-js",
    "@algolia/autocomplete-plugin-algolia-insights": "https://pkg.csb.dev/algolia/autocomplete.js/commit/f945a87e/@algolia/autocomplete-plugin-algolia-insights",
    "@algolia/autocomplete-preset-algolia": "https://pkg.csb.dev/algolia/autocomplete.js/commit/f945a87e/@algolia/autocomplete-preset-algolia",
    "@algolia/autocomplete-theme-classic": "https://pkg.csb.dev/algolia/autocomplete.js/commit/f945a87e/@algolia/autocomplete-theme-classic",
    "@algolia/client-search": "4.8.3",
    "algoliasearch": "4.8.3",
    "preact": "^10.2.1",
    "preact-compat": "3.19.0",
    "react-instantsearch-dom": "6.9.0"
  },
  "jest": {
    "verbose": true,
    "setupFiles": [
      "<rootDir>/src/tests/__mocks__/browserMocks.js"
    ],
    "testURL": "http://localhost:8080",
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleDirectories": [
      "node_modules"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/src/tests/__mocks__/fileMock.js",
      "\\.(css|less|scss)$": "identity-obj-proxy",
      "^./style$": "identity-obj-proxy",
      "^preact$": "<rootDir>/node_modules/preact/dist/preact.min.js",
      "^react$": "preact-compat",
      "^react-dom$": "preact-compat",
      "^create-react-class$": "preact-compat/lib/create-react-class",
      "^react-addons-css-transition-group$": "preact-css-transition-group"
    }
  },
  "keywords": [
    "preact",
    "starter"
  ],
  "description": "Preact example starter project"
}